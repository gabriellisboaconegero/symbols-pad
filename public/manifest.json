{
  "name": "Symbols tab",
  "description": "Extension for writing symbols, mathematics, logic, etc.",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Symbols pad"
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}
